CREATE TABLE gs_table_v1( 
	Date_Loaded DATETIME2 NOT NULL DEFAULT GETDATE(),
	nameWithOwner NVARCHAR(200),
	stargazers INT,
	createdAt DATETIME2,
	updatedAt DATETIME2,
	openissues INT,
	closedissues INT,
	forkCount INT,
	pullRequests INT,
	commitnum INT,
	watchers INT,
	diskUsage_kb INT,
	readme_bytes INT,
	releases INT,
	projects INT,
	milestones INT,
	deployments INT,
	primaryLanguage NVARCHAR(200),
	issuelabels INT,
	repositoryTopics INT,
	licenseInfo NVARCHAR(2000),
	homepageUrl NVARCHAR(1000),
	description NVARCHAR(2000),
	hasIssuesEnabled BIT,
	hasWikiEnabled BIT,
	isLocked BIT,
	isDisabled BIT,
	createdAt_sec INT,
	updatedAt_sec INT,
	id NVARCHAR(60),
	databaseId INT
);
